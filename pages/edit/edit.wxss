page {
  width: 100%;
  height: 100%;
}
.edit-container {
  width: 100%;
  height: 100%;
  padding: 40rpx 40rpx;
  box-sizing: border-box;
  background: #eee;
  display: flex;
  flex-direction: column;
}
.content-box {
  flex: 1;
  overflow-y: auto;
  -webkit-overflow-scrolling: touch;
}
.title-container, .content-list {
  width: 100%;
  height: 100rpx;
  padding: 20rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  background: #fff;
  border-radius: 14rpx;
}
.icon-title {
  width: 48rpx;
  height: 48rpx;
  margin-right: 30rpx;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100% 100%;
}
.input-wrapper {
  flex: 1;
  height: 100%;
}
.input-ele {
  width: 100%;
  height: 100%;
}
.content-list {
  margin-top: 40rpx;
}
.icon-delete {
  width: 48rpx;
  height: 48rpx;
  margin-right: 30rpx;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100% 100%;
}
.add-new-btn {
  width: 100%;
  height: 100rpx;
  padding: 20rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  background: #fff;
  border-radius: 14rpx;
  margin-top: 40rpx;
}
.add-icon {
  width: 48rpx;
  height: 48rpx;
  margin-right: 30rpx;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100% 100%;
}
.tips {
  font-size: 32rpx;
}
.confirm-btn-wrapper {
  width: 100%;
  height: 120rpx;
  padding-bottom: 20rpx;
  padding-bottom: calc(20rpx + env(safe-area-inset-bottom));
  padding-bottom: calc(20rpx + constant(safe-area-inset-bottom));
  box-sizing: border-box;
}
.confirm-btn {
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  background: #FFC300;
  color: #fff;
  font-size: 40rpx;
  border-radius: 14rpx;
}