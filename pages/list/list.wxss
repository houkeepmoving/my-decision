page {
  width: 100%;
  height: 100%;
}
.list-container { 
  width: 100%;
  height: 100%;
  padding: 0 40rpx;
  box-sizing: border-box;
  padding-top: 40rpx;
}
.list-wrapper {
  display: flex;
  width: 100%;
  padding: 20rpx;
  box-sizing: border-box;
  height: 100rpx;
  align-items: center;
  justify-content: space-between;
  background: #fff;
  margin-bottom: 40rpx;
  border-radius: 14rpx;
}
.list-wrapper:last-of-type {
  margin-bottom: 0;
}
.left-content {
  height: 100%;
  font-size: 32rpx;
  color: #333;
  flex: 1;
  display: flex;
  align-items: center;
}
.edit-icon {
  width: 48rpx;
  height: 48rpx;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100% 100%;
}
.add-new-btn {
  width: 88rpx;
  height: 88rpx;
  position: fixed;
  right: 60rpx;
  bottom: 200rpx;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100% 100%;
}