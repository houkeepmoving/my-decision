page {
  width: 100%;
  height: 100%;
}
.list-container { 
  width: 100%;
  height: 100%;
  padding: 0 40rpx;
  box-sizing: border-box;
  padding-top: 40rpx;
}
.list-wrapper {
  display: flex;
  width: 100%;
  padding: 20rpx;
  box-sizing: border-box;
  height: 100rpx;
  align-items: center;
  justify-content: space-between;
  background: #fff;
  margin-bottom: 40rpx;
  border-radius: 14rpx;
}
.list-wrapper:last-of-type {
  margin-bottom: 0;
}
.left-content {
  height: 100%;
  font-size: 32rpx;
  color: #333;
  flex: 1;
  display: flex;
  align-items: center;
}
.edit-icon {
  width: 48rpx;
  height: 48rpx;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100% 100%;
}
.add-new-btn {
  width: 80rpx;
  height: 80rpx;
  position: fixed;
  right: 80rpx;
  bottom: 300rpx;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100% 100%;
}
.no-data-wrapper {
  width: 100%;
  margin-top: 200rpx;
}
.no-data-icon {
  width: 392rpx;
  height: 256rpx;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100% 100%;
  margin: 0 auto 40rpx auto;
}
.no-data-tips {
  width: 100%;
  text-align: center;
  color: #707070;
  font-size: 30rpx;
  margin-bottom: 20rpx;
  line-height: 1;
}
.add-btn {
  width: 160rpx;
  margin: 0 auto;
  padding: 16rpx;
  color: #333;
  line-height: 1;
  font-size: 36rpx;
  font-weight: 600;
  background-color: #FFC300;
  border-radius: 16rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}