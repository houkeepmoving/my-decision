page {
  width: 100%;
  height: 100%;
}
.template-container { 
  width: 100%;
  height: 100%;
  padding: 0 40rpx;
  box-sizing: border-box;
  background: #eee;
  padding-top: 40rpx;
}
.common-wrapper {
  margin-top: 20rpx;
}
.title {
  width: 100%;
  text-indent: 20rpx;
  color: #707070;
  font-size: 32rpx;
  margin-bottom: 20rpx;
}
.icon {
  width: 48rpx;
  height: 48rpx;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100% 100%;
  margin-right: 10rpx;
}
.make-new-list {
  width: 100%;
  height: 100rpx;
  display: flex;
  align-items: center;
  padding: 20rpx;
  box-sizing: border-box;
  font-size: 32rpx;
  color: #333;
  background: #fff;
  border-radius: 14rpx;
  margin-top: 40rpx;
}
.make-new {
  color: #1296db;
}